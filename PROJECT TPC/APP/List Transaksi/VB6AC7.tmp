                        time_out = Format(DateAdd("d", 1, .CellText(aa, 0)), "yyyy-MM-dd") & " " & Format(.CellText(aa, 7), "hh:mm") & ":00"
                    Else
                        time_out = Format(.CellText(aa, 0), "yyyy-MM-dd") & " " & Format(.CellText(aa, 7), "hh:mm") & ":00"
                    End If
                    
                    rs.Close
    
                    If .CellText(aa, 0) <> "" And .CellText(aa, 1) <> "" Then
